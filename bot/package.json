{
  "name": "codefair",
  "version": "1.0.0",
  "private": true,
  "description": "Checks for License and Citation in a Github repo",
  "author": "slugb0t",
  "license": "MIT",
  "homepage": "https://github.com//",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon --watch ./main.js --watch .env --exec \" node -r dotenv/config ./main.js\"",
    "build-css": "tailwindcss build -i ./public/assets/css/tailwind.css -o ./public/assets/css/styles.css",
    "watch-css": "npx tailwindcss build -i ./public/assets/css/tailwind.css -o ./public/assets/css/styles.css --watch",
    "build": "npm run build-css",
    "test": "jest"
  },
  "dependencies": {
    "@probot/pino": "^2.3.5",
    "axios": "^1.6.8",
    "dotenv": "^16.4.5",
    "humanparser": "^2.7.0",
    "js-yaml": "^4.1.0",
    "mongodb": "^6.5.0",
    "nanoid": "^5.0.7",
    "pino": "^9.1.0",
    "probot": "13.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "jest": "^29.0.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.38",
    "smee-client": "^2.0.0",
    "tailwindcss": "^3.4.3"
  },
  "engines": {
    "node": ">= 18"
  },
  "type": "module",
  "jest": {
    "testEnvironment": "node"
  }
}
